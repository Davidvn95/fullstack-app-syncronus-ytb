{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "no-unused-vars": [1, { "args": "after-used", "argsIgnorePattern": "^_" }]
  }
}

/*
"off" or 0 - turn the role off
"warn" or 1 - turn the rule on as a warning (doesn't affect exist code)
"error" or 2 - turn the rule on as an error (exist code will be 1)
*/
